@if (DateTime.UtcNow.Month == 6)
{
    <link rel="icon" type="image/svg+xml" href="/pride/silverbot.svg">
}
else if (DateTime.UtcNow is { Month: 10, Day: 31 })
{
    <link rel="icon" type="image/svg+xml" href="/halloween/silverbot.svg">
}
else
{
    <link rel="icon" type="image/svg+xml" href="/silverbot.svg">
}