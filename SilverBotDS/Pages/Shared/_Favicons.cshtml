@if (DateTime.UtcNow.Month == 6)
{
    <link rel="apple-touch-icon" sizes="180x180" href="/pride/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/pride/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/pride/favicon-16x16.png">
    <link rel="manifest" href="/pride/site.webmanifest">
    <link rel="mask-icon" href="/pride/safari-pinned-tab.svg" color="#34f0fd">
    <link rel="shortcut icon" href="/pride/favicon.ico">
    <meta name="msapplication-TileColor" content="#34f0fd">
    <meta name="msapplication-config" content="/pride/browserconfig.xml">
    <meta name="theme-color" content="#34f0fd">
}
else if (DateTime.UtcNow.Month == 10 && DateTime.UtcNow.Day == 31)
{
    <link rel="apple-touch-icon" sizes="180x180" href="/halloween/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/halloween/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/halloween/favicon-16x16.png">
    <link rel="manifest" href="/halloween/site.webmanifest">
    <link rel="mask-icon" href="/halloween/safari-pinned-tab.svg" color="#5bbad5">
    <link rel="shortcut icon" href="/halloween/favicon.ico">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-config" content="/halloween/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
}
else
{
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png?v=69">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png?v=69">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png?v=69">
    <link rel="manifest" href="/site.webmanifest?v=69">
    <link rel="mask-icon" href="/safari-pinned-tab.svg?v=69" color="#34f0fd">
    <link rel="shortcut icon" href="/favicon.ico?v=69">
    <meta name="msapplication-TileColor" content="#34f0fd">
    <meta name="msapplication-TileImage" content="/mstile-144x144.png?v=69">
    <meta name="theme-color" content="#34f0fd">
}